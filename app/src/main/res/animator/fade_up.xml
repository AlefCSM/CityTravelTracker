    <objectAnimator
        android:duration="1200"
        android:propertyName="alpha"
        android:startOffset="300"
        android:valueFrom="1"
        android:valueTo="0.5"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="1200"
        android:propertyName="translateY"
        android:valueFrom="0"
        android:valueTo="100"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="1200"
        android:propertyName="translateZ"
        android:valueFrom="1"
        android:valueTo="0"
        android:valueType="floatType" />
</set>
